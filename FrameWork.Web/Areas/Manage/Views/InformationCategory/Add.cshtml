@{
    ViewBag.Title = "新增资讯类型";
    Layout = "~/Areas/Manage/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <p></p>
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">新增资讯类型</h3>
                </div>
                <div class="panel-body">
                    @using (Html.BeginForm())
            {
                        @Html.AntiForgeryToken()



                        <div class="form-group">
                            <label for="txtId">编号</label>
                            <input type="text" class="form-control" id="txtId" placeholder="id" name="id">
                        </div>
                        <div class="form-group">
                            <label for="txtName">名称</label>
                            <input type="text" class="form-control" id="txtName" placeholder="名称" name="name">
                        </div>

                        <button type="submit" class="btn btn-primary">新增</button>
                        <button type="button" class="btn btn-default" onclick="javascript:window.location.href='@Url.Action("Index","InformationCategory")'">返回</button>
                    }
                </div>
            </div>
        </div>
    </div>
</div>

